digraph finite_state_machine {
	0 [shape=circle]
	1 [shape=circle]
	2 [shape=circle]
	3 [shape=circle]
	4 [shape=circle]
	5 [shape=circle]
	6 [shape=circle]
	7 [shape=circle]
	6 [shape=doublecircle]
	7 [shape=doublecircle]
	0 -> 2 [label=p1]
	0 -> 3 [label=p2]
	2 -> 6 [label=p4]
	3 -> 7 [label=p6]
	2 -> 1 [label=p3]
	3 -> 1 [label=p5]
	1 -> 5 [label=p7]
	1 -> 4 [label=p8]
	5 -> 7 [label=p10]
	5 -> 0 [label=p12]
	4 -> 6 [label=p9]
	4 -> 0 [label=p11]
}
