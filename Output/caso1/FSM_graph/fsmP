digraph finite_state_machine {
	fg [shape=circle]
	fs [shape=circle]
	sv [shape=circle]
	sp [shape=circle]
	gv [shape=circle]
	gp [shape=circle]
	fmv [shape=circle]
	fmp [shape=circle]
	fmv [shape=doublecircle]
	fmp [shape=doublecircle]
	fg -> sv [label=p1]
	fg -> sp [label=p2]
	sv -> fmv [label=p4]
	sp -> fmp [label=p6]
	sv -> fs [label=p3]
	sp -> fs [label=p5]
	fs -> gp [label=p7]
	fs -> gv [label=p8]
	gp -> fmp [label=p10]
	gp -> fg [label=p12]
	gv -> fmv [label=p9]
	gv -> fg [label=p11]
}
